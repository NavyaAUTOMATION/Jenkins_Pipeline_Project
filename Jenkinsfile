pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World This is Navya'
            }
        }
        stage('Build') {
            steps {
                echo 'Building in Process'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the file'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
